<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.ass.mcoerctest">

    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />
    <uses-permission android:name="android.permission.RECEIVE_BOOT_COMPLETED" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/mcoerc_logo"
        android:label="@string/app_name"
        android:networkSecurityConfig="@xml/network_security_config"
        android:roundIcon="@mipmap/mcoerc_logo"
        android:supportsRtl="true"
        android:theme="@style/Theme.Mhcet">
        <activity android:name="com.ass.mcoerctest.ScoreCardActivity"></activity>
        <activity android:name="com.ass.mcoerctest.TestActivity" />
        <activity android:name="com.ass.mcoerctest.TestListActivity" />
        <activity android:name="com.ass.mcoerctest.TestsDashboardActivity" />
        <activity android:name="com.ass.mcoerctest.ChaptersActivity" />
        <activity android:name="com.ass.mcoerctest.AnswerActivity" />
        <activity android:name="com.ass.mcoerctest.QuestionsActivity" />
        <activity android:name="com.ass.mcoerctest.SplashActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.ass.mcoerctest.LoginActivity" />
        <activity
            android:name="com.ass.mcoerctest.DashboardActivity"
            android:theme="@style/Theme.Mhcet.NoActionBar" />
        <activity android:name="com.ass.mcoerctest.PhoneAuthenticationActivity" />
    </application>

</manifest>